{
  "name": "trees-items-service",
  "version": "0.1.2",
  "description": "",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "init": "yarn",
    "test": "mocha --exit --recursive test --require babel-register",
    "build": "rimraf index.js && npm run test && babel src --out-dir .",
    "prepublishOnly": "npm version patch && npm run build",
    "npmpublish": "npm publish",
    "up-errors": "yarn upgrade -L trees-errors",
    "up-query": "yarn upgrade -L trees-query",
    "up-units": "yarn upgrade -L trees-units",
    "up-all": "npm run up-errors && npm run up-query && npm run up-units"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlueForestTrees/trees-items-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlueForestTrees/trees-items-service/issues"
  },
  "homepage": "https://github.com/BlueForestTrees/trees-items-service#readme",
  "dependencies": {
    "trees-errors": "^0.0.0",
    "trees-query": "^0.0.9",
    "trees-units": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.3",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  }
}
